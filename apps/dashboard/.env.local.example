# Base URL delle API esposte dal server Express
VITE_API_BASE_URL=http://localhost:3333/api

# Token di accesso per le chiamate dal frontend
VITE_API_TOKEN=replace-with-your-token

# Identificativo utente da inviare nelle richieste autenticate
VITE_API_USER=replace-with-your-user

# Ruoli (comma-separated) da inviare per consentire operazioni protette
VITE_API_ROLES=taxonomy:manage
